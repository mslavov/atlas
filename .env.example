# Next.js Configuration
NODE_ENV=
NEXT_PUBLIC_API_URL=
NEXT_PUBLIC_APP_NAME=

# Nango Configuration (Required)
# Get the secret key from https://app.nango.dev/environment-settings
NANGO_SECRET_KEY=
NANGO_WEBHOOK_SECRET=
NANGO_HOST=https://api.nango.dev

# Zep Configuration (Required)
# Get your API key from https://app.getzep.com
ZEP_API_KEY=

# Project Configuration (Optional)
DEFAULT_PROJECT_ID=atlas

# Security (Required for Production)
NEXTAUTH_SECRET=
NEXTAUTH_URL=

# Database (Optional but Recommended)
DATABASE_URL=

# Monitoring (Optional)
SENTRY_DSN=
DATADOG_API_KEY=